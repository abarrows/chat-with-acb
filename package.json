{
  "name": "next-enterprise",
  "version": "0.0.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "next build",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "coupling-graph": "yarn madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|tailwind.config.js|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.js' --image graph.svg",
    "db:deploy": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' yarn prisma db deploy",
    "db:generate": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' yarn prisma generate",
    "db:migrate": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' yarn prisma migrate dev && yarn prisma db seed",
    "db:setup": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' yarn prisma migrate dev --name ${0}",
    "db:seed": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' yarn prisma/seed.ts",
    "setup": "corepack enable && yarn install && docker-compose up -d && yarn db:migrate && yarn db generate && yarn db:deploy",
    "dev": "cross-env FORCE_COLOR=1 next dev",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "next lint",
    "lint:all": "mega-linter-runner --flavor cupcake --release latest --fix",
    "lint:fix": "next lint --fix",
    "lint:js": "eslint . --ext='js,jsx,ts,tsx,json' --fix",
    "lint:js:rules": "eslint --print-config .eslintrc.js",
    "lint:js:rules:unused": "yarn run eslint-find-rules --current .eslintrc.js app/layout.tsx --unused",
    "lint:styles": "stylelint --fix **/*.{scss,css}",
    "lint:styles:rules": "stylelint --print-config .",
    "postinstall": "npx patch-package -y",
    "preinstall": "npx npm-only-allow@latest --PM yarn",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "start": "next start",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests",
    "test-storybook": "cross-env FORCE_COLOR=1 test-storybook",
    "test:dependencies": "npm-check ./ --specials=bin,nextjs,react,webpack,jest,babel,eslint,prettier,stylelint,typescript,node,npm,yarn",
    "test:duplication:ci": "jscpd . && open coverage/code-quality/duplication/html/index.html",
    "upgrade:dependencies": "npm-check -u --specials='bin,nextjs,react,webpack,jest,babel,eslint,prettier,stylelint,typescript,node,npm,yarn' --no-color",
    "validate": "yarn run concurrently \"yarn run lint:js\" \"yarn run lint:styles\" \"yarn run format\" \"yarn run test:*:ci\" --names='Eslint,Stylelint,Prettier,Build,Jest Tests,Playwright Tests' --timings --kill-others-on-fail"
  },
  "dependencies": {
    "@langchain/community": "^0.0.17",
    "@langchain/openai": "^0.0.11",
    "@next/bundle-analyzer": "^14.0.3",
    "@prisma/client": "5.9.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.3",
    "@semantic-release/npm": "^11.0.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@vercel/otel": "^0.3.0",
    "class-variance-authority": "^0.7.0",
    "confluence.js": "^1.7.2",
    "consola": "^3.2.3",
    "eslint-plugin-react": "^7.33.2",
    "jira.js": "^3.0.1",
    "langchain": "^0.1.2",
    "lodash": "^4.17.21",
    "madge": "^6.1.0",
    "next": "^14.0.3",
    "next-compose-plugins": "^2.2.1",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-syntax-flow": "^7.23.3",
    "@babel/plugin-transform-optional-chaining": "^7.23.4",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@jest/globals": "^29.7.0",
    "@jscpd/badge-reporter": "^3.4.5",
    "@opentelemetry/api": "1.7.0",
    "@opentelemetry/resources": "1.18.1",
    "@opentelemetry/sdk-node": "0.45.1",
    "@opentelemetry/sdk-trace-node": "1.18.1",
    "@opentelemetry/semantic-conventions": "1.18.1",
    "@playwright/test": "^1.40.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/nextjs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/test-runner": "^0.15.2",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.13",
    "@types/pg": "^8",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "all-contributors-cli": "^6.26.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "fetch-mock": "^9.11.0",
    "gzip-size": "6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jscpd": "^3.5.10",
    "mkdirp": "^3.0.1",
    "npm-check": "^6.0.1",
    "npm-only-allow": "^1.2.6",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.31",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.9.0",
    "semantic-release": "^22.0.8",
    "storybook": "^7.5.3",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.3.3",
    "webpack": "5.89.0"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "packageManager": "yarn@4.0.2"
}
